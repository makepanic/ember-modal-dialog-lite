{
  "name": "@makepanic/ember-modal-dialog-lite",
  "version": "0.1.0",
  "description": "An ember-cli addon for implementing modal dialogs",
  "keywords": [
    "ember-addon"
  ],
  "private": false,
  "repository": "https://github.com/makepanic/ember-modal-dialog-lite",
  "license": "MIT",
  "author": "Yapp Labs and friends",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint ./*.js app config lib server tests",
    "start": "ember serve",
    "test": "ember test",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "test:all": "ember try:each",
    "lint:hbs": "ember-template-lint .",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
